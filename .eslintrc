{
  "extends": [
    "airbnb-base"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": [
        "@babel/plugin-syntax-import-assertions"
      ]
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      }
    }
  },
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "jsx-a11y/img-has-alt": 0,
    "default-case": 0,
    "func-names": 0,
    "consistent-return": 0,
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
    "no-console": 0,
    "object-curly-newline": 0,
    "array-bracket-spacing": ["error", "always", {
      "singleValue": true,
      "objectsInArrays": false,
      "arraysInArrays": false
    }],
    "no-restricted-syntax":0,
    "comma-dangle": 0,
    "padded-blocks": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-tabs": 0
  }
}